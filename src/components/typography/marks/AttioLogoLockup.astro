<style>
  .inline-mark {
    position: relative;
    display: inline-block;
    height: var(--text-base);
    width: auto;
    padding-inline: var(--space-05);
    vertical-align: baseline;
    transform: translateY(var(--space-05));
    color: var(--fg);
    text-decoration: none;
    border: none;
    transition:
      color var(--transition-fast),
      transform var(--transition-fast);
  }

  .inline-mark::after {
    content: "";
    position: absolute;
    bottom: calc(-1 * var(--space-1));
    left: var(--space-05);
    right: var(--space-05);
    height: var(--border-hairline);
    background: var(--emphasis);
    transform: scaleX(0);
    transform-origin: left;
    transition:
      transform var(--transition-fast),
      background var(--transition-fast);
  }

  .inline-mark:hover {
    color: var(--emphasis);
    transform: translateY(0);
  }

  .inline-mark:hover::after {
    transform: scaleX(1);
  }

  .inline-mark:focus-visible {
    outline: var(--outline-width) solid var(--fg);
    outline-offset: var(--outline-offset);
  }

  .inline-mark svg {
    height: 100%;
    width: auto;
  }
</style>
